        &gt;&gt;&gt; t('$def with (d)\n$for k, v in d.iteritems(): $k')({1: 1})
        u'1\n'
        &gt;&gt;&gt; t('$for x in [1, 2, 3]:\n\t$x')()
        u'    1\n    2\n    3\n'
        &gt;&gt;&gt; t('$def with (a)\n$while a and a.pop():1')([1, 2, 3])
        u'1\n1\n1\n'

    The space after : must be ignored.

        &gt;&gt;&gt; t('$if True: foo')()
        u'foo\n'

