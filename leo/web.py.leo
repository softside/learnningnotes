        &gt;&gt;&gt; python_lookahead = Parser('').python_lookahead
        &gt;&gt;&gt; python_lookahead('for i in range(10):')
        'for'
        &gt;&gt;&gt; python_lookahead('else:')
        'else'
        &gt;&gt;&gt; python_lookahead(' x = 1')
